<yas>
	<scene root="court" axis_length="3" />
	<views default="perspective">
        <perspective id="perspective" near="0.1" far="500.0" angle="25.0">
            <from x="100.0" y="100.0" z="100.0" />
            <to x="0.0" y="0.0" z="0.0" />
        </perspective>
    </views>
	<ambient>
 		<ambient r="0.5" g="0.5" b="0.5" a="1.0" />
 		<background r="0.529412" g="0.692237" b="0.921569" a="1.0" />
	</ambient>
 	<lights>
	    <omni id="light0" enabled="1" >
            <location x="4" y="6" z="1" w="1" />
            <ambient r="0" g="0" b="0" a="1" />
            <diffuse r="1" g="1" b="1" a="1" />
            <specular r="1" g="1" b="0" a="1" />
        </omni>
        <omni id="light1" enabled="1" >
            <location x="10.5" y="6" z="1" w="1" />
            <ambient r="0" g="0" b="0" a="1" />
            <diffuse r="1" g="1" b="1" a="1" />
            <specular r="0.5" g="0.5" b="0.5" a="1" />
        </omni>
        <omni id="light2" enabled="1" >
            <location x="10.5" y="6" z="5" w="1" />
            <ambient r="0" g="0" b="0" a="1" />
            <diffuse r="1" g="1" b="1" a="1" />
            <specular r="1" g="1" b="1" a="1" />
        </omni>
 	</lights>
	<textures> 
 		<texture id="courttexture" file="images/court.jpg" />
 		<texture id="balltexture" file="images/ball.jpg" />
 		<texture id="backboard" file="images/backboard.jpg" />
 	</textures>
	<materials>
 		<material id="defmaterial" shininess = "1" >
 			<emission r="0" g="0" b="0" a="1" />
 			<ambient r="1" g="1" b="1" a="1" />
 			<diffuse r="0" g="0" b="0" a="1" />
 			<specular r="0" g="0" b="0" a="0" />
 		</material>
		<material id="rimmaterial" shininess = "1" >
			<emission r="0" g="0" b="0" a="0" />
 			<ambient r="1" g="0.25" b="0" a="1" />
 			<diffuse r="0" g="0" b="0" a="0" />
 			<specular r="0" g="0" b="0" a="0" />
 		</material>
	 	<material id="suportematerial" shininess = "1" >
			<emission r="0" g="0" b="0" a="0" />
 			<ambient r="1" g="1" b="1" a="1" />
 			<diffuse r="0" g="0" b="0" a="0" />
 			<specular r="0" g="0" b="0" a="0" />
 		</material>
		<material id="scorematerial" shininess = "1" >
 			<emission r="0" g="0" b="0" a="0" />
 			<ambient r="0" g="0" b="0" a="1" />
 			<diffuse r="0" g="0" b="0" a="0" />
 			<specular r="0" g="0" b="0" a="0" />
 		</material>
 	</materials>
	<transformations>
		<transformation id="transf1">
			<scale x="1" y="1" z="1"/>
		</transformation>
	</transformations>
	<primitives>
 		<primitive id="square">
 			<rectangle x1="-0.5" y1="-0.5" x2="0.5" y2="0.5" />
 		</primitive >
		<primitive id="cylinder">
			<cylinder base="1" top="1" height="1" slices="1" stacks="1" />
		</primitive >
		<primitive id="triangle">
 			<triangle x1="0" y1="-0.5" z1="0" x2="0.5" y2="0.5" z2="0" x3="-0.5" y3="0.5" z3="0" />
		</primitive >
		<primitive id="sphere">
 			<sphere radius="0.5" slices="1" stacks="1" />
		</primitive >
		<primitive id="torus">
			<torus inner="0.45" outer="0.5" slices="1" loops="1" />
		</primitive >
	</primitives >
	<components>
		<component id="face1">
	 		<transformation>
	 			<translate x="0" y="0" z="0.5" />
	 		</transformation>
			<materials>
	 			<material id="inherit" />
	 		</materials>
	 		<texture id="none" length_s="1.0" length_t="1.0"/>
	 		<children>
	 			<primitiveref id="square" />
	 		</children>
	 	</component>
 		<component id="face2">
	 		<transformation>
	 			<translate x="0" y="0" z="-0.5" />
	 			<rotate axis="y" angle="180" />
			</transformation>
	 		<materials>
	 			<material id="inherit" />
	 		</materials>
			<texture id="none" length_s="1.0" length_t="1.0"/>
	 		<children>
	 			<primitiveref id="square" />
	 		</children>
		</component>
		<component id="face3">
			<transformation>
				<translate x="0" y="0.5" z="0" />
				<rotate axis="x" angle="-90" />
			</transformation>
			<materials>
				<material id="inherit" />
			</materials>
			<texture id="none" length_s="1.0" length_t="1.0"/>
			<children>
				<primitiveref id="square" />
			</children>
		</component>
		<component id="face4">
			<transformation>
				<translate x="0" y="-0.5" z="0" />
				<rotate axis="x" angle="90" />
			</transformation>
			<materials>
				<material id="inherit" />
			</materials>
			<texture id="none" length_s="1.0" length_t="1.0"/>
			<children>
				<primitiveref id="square" />
			</children>
		</component>
		<component id="face5">
			<transformation>
				<translate x="0.5" y="0" z="0" />
				<rotate axis="y" angle="90" />
			</transformation>
			<materials>
				<material id="inherit" />
			</materials>
			<texture id="none" length_s="1.0" length_t="1.0"/>
			<children>
				<primitiveref id="square" />
			</children>
		</component>
		<component id="face6">
			<transformation>
				<translate x="-0.5" y="0" z="0" />
				<rotate axis="y" angle="-90" />
			</transformation>
			<materials>
				<material id="inherit" />
			</materials>
			<texture id="none" length_s="1.0" length_t="1.0"/>
			<children>
				<primitiveref id="square" />
			</children>
		</component>
		<component id="cube">
			<transformation>
				<transformationref id="transf1" />
			</transformation>
			<materials>
				<material id="inherit" />
			</materials>
			<texture id="none" length_s="1.0" length_t="1.0"/>
			<children>
				<componentref id="face1" />
				<componentref id="face2" />
				<componentref id="face3" />
				<componentref id="face4" />
				<componentref id="face5" />
				<componentref id="face6" />
			</children>
		</component>
		<!-- Bola de Basket. -->
		<component id="basketball">
			<transformation>
				<translate x="-8" y="3.7" z="2" />
				<scale x="0.25" y="0.25" z="0.25" />
			</transformation>
			<materials>
				<material id="defmaterial" />
			</materials>
			<texture id="balltexture" length_s="1.0" length_t="1.0"/>
			<children>
				<primitiveref id="sphere" />
			</children>
		</component>
		<!-- ChÃ£o do campo. -->
		<component id="courtfloor">
			<transformation>
				<scale x="28" y="0.1" z="15" />
			</transformation>
			<materials>
				<material id="defmaterial" />
			</materials>
			<texture id="courttexture" length_s="1.0" length_t="1.0"/>
			<children>
				<componentref id="cube" />
			</children>
		</component>
		<!-- Aro do cesto. -->
		<component id="aro">
			<transformation>
				<translate x="-10.2" y="2.7" z="0" />
				<scale x="0.3" y="0.1" z="0.3" />
				<rotate axis="x" angle="90" />
			</transformation>
			<materials>
				<material id="rimmaterial" />
			</materials>
			<texture id="none"  length_s="1.0" length_t="1.0"/>
			<children>
				<primitiveref id="torus" />
			</children>
		</component>
		<!-- Tabela do cesto. -->
		<component id="tabela">
			<transformation>
				<translate x="-10.5" y="2.95" z="0" />
				<scale x="0.1" y="1.05" z="1.7" />
			</transformation>
			<materials>
				<material id="defmaterial" />
			</materials>
			<texture id="backboard" length_s="1.0" length_t="1.0"/>
			<children>
				<componentref id="cube" />
			</children>
		</component>
		<!-- Cesto. -->
		<component id="cesto">
			<transformation>
				<transformationref id="transf1" />
			</transformation>
			<materials>
				<material id="inherit" />
			</materials>
			<texture id="none" length_s="1.0" length_t="1.0"/>
			<children>
				<componentref id="tabela" />
				<componentref id="aro" />
			</children>
		</component>
		<!-- Suporte 1 do Cesto. -->
		<component id="suportecesto1">
			<transformation>
				<translate x="-11" y="1.525" z="0" />
				<scale x="0.2" y="3.05" z="0.2" />
			</transformation>
			<materials>
				<material id="suportematerial" />
			</materials>
			<texture id="none" length_s="1.0" length_t="1.0"/>
			<children>
				<componentref id="cube" />
			</children>
		</component>
		<!-- Suporte 2 do Cesto. -->
		<component id="suportecesto2">
			<transformation>
				<translate x="-10.75" y="2.95" z="0" />
				<scale x="0.5" y="0.2" z="0.2" />
			</transformation>
			<materials>
				<material id="suportematerial" />
			</materials>
			<texture id="none" length_s="1.0" length_t="1.0"/>
			<children>
				<componentref id="cube" />
			</children>
		</component>
		<!-- Suporte do Cesto total. -->
		<component id="suportecestototal">
			<transformation>
				<transformationref id="transf1" />
			</transformation>
			<materials>
				<material id="inherit" />
			</materials>
			<texture id="none" length_s="1.0" length_t="1.0"/>
			<children>
				<componentref id="suportecesto1" />
				<componentref id="suportecesto2" />
			</children>
		</component>
		<!-- Cesto Total 1. -->
		<component id="cestototal1">
			<transformation>
				<transformationref id="transf1" />
			</transformation>
			<materials>
				<material id="defmaterial" />
			</materials>
			<texture id="none" length_s="1.0" length_t="1.0"/>
			<children>
				<componentref id="cesto" />
				<componentref id="suportecestototal" />
			</children>
		</component>
		<!-- Cesto Total 2. -->
		<component id="cestototal2">
			<transformation>
				<rotate axis="y" angle="180" />
			</transformation>
			<materials>
				<material id="defmaterial" />
			</materials>
			<texture id="none" length_s="1.0" length_t="1.0"/>
			<children>
				<componentref id="cesto" />
				<componentref id="suportecestototal" />
			</children>
		</component>
		<!-- Os dois cestos. -->
		<component id="cestos">
			<transformation>
				<transformationref id="transf1" />
			</transformation>
			<materials>
				<material id="inherit" />
			</materials>
			<texture id="none" length_s="1.0" length_t="1.0"/>
			<children>
				<componentref id="cestototal1" />
				<componentref id="cestototal2" />
			</children>
		</component>
		<component id="triangulo1">
			<transformation>
				<translate x="0" y="0" z="1.5" />
			</transformation>
			<materials>
				<material id="inherit" />
			</materials>
			<texture id="none" length_s="1.0" length_t="1.0"/>
			<children>
				<primitiveref id="triangle" />
			</children>
		</component>		
		<component id="triangulo2">
			<transformation>
				<rotate axis="y" angle="180" />
			</transformation>
			<materials>
				<material id="inherit" />
			</materials>
			<texture id="none" length_s="1.0" length_t="1.0"/>
			<children>
				<componentref id="triangulo1" />
			</children>
		</component>
		<component id="quadrado1">
			<transformation>
				<scale x="1.0" y="1.0" z="3" />
				<translate x="0" y="0.5" z="0" />
				<rotate axis="x" angle="-90" />
			</transformation>
			<materials>
				<material id="inherit" />
			</materials>
			<texture id="none" length_s="1.0" length_t="1.0"/>
			<children>
				<primitiveref id="square" />
			</children>
		</component>
		<component id="quadrado2">
			<transformation>
				<scale x="1.11803399" y="1.11803399" z="3" />
				<translate x="0.22" y="0" z="0" />
				<rotate axis="y" angle="90" />
				<rotate axis="x" angle="26.56505118" />
			</transformation>
			<materials>
				<material id="inherit" />
			</materials>
			<texture id="none" length_s="1.0" length_t="1.0"/>
			<children>
				<primitiveref id="square" />
			</children>
		</component>
		<component id="quadrado3">
			<transformation>
				<scale x="1.11803399" y="1.11803399" z="3" />
				<translate x="-0.22" y="0" z="0" />
				<rotate axis="y" angle="-90" />
				<rotate axis="x" angle="26.56505118" />
			</transformation>
			<materials>
				<material id="inherit" />
			</materials>
			<texture id="none" length_s="1.0" length_t="1.0"/>
			<children>
				<primitiveref id="square" />
			</children>
		</component>
		<!-- scoreboard -->
		<component id="scoreboard">
			<transformation>
				<scale x="1.5" y="1.5" z="1" />
				<translate x="0" y="6" z="0" />
			</transformation>
			<materials>
				<material id="scorematerial" />
			</materials>
			<texture id="none" length_s="1.0" length_t="1.0"/>
			<children>
				<componentref id="triangulo1" />
				<componentref id="triangulo2" />
				<componentref id="quadrado1" />
				<componentref id="quadrado2" />
				<componentref id="quadrado3" />
			</children>
		</component>
		<!-- Campo de Basket -->
		<component id="court">
			<transformation>
				<transformationref id="transf1" />
			</transformation>
			<materials>
				<material id="inherit" />
			</materials>
			<texture id="none" length_s="1.0" length_t="1.0"/>	
			<children>
				<componentref id="cestos" />
				<componentref id="courtfloor" />
				<componentref id="scoreboard" />
			</children>
		</component>
	</components>
</yas>