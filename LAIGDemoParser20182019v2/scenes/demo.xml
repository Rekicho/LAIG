		<perspective id="front" near="0.1" far="100.0" angle="45.0">
			<from x="0.0" y="10.0" z="25.0" />
			<to x="0.0" y="0.0" z="0.0" />
		</perspective>
		<perspective id="back" near="0.1" far="100.0" angle="45.0">
			<from x="0.0" y="10.0" z="-25.0" />
			<to x="0.0" y="0.0" z="0.0" />
		</perspective>
		<perspective id="side" near="0.1" far="100.0" angle="45.0">
			<from x="25.0" y="10.0" z="0.0" />
			<to x="0.0" y="0.0" z="0.0" />
		</perspective>
		<ortho id="ortho" near="0.1" far="100.0" left="-30.0" right="30.0" top="10.0" bottom="-10.0">
